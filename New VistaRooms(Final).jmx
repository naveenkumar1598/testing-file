if(rc.equals(&quot;200&quot;)){
	bw.newLine();
	bw.newLine();
 bw.write(&quot;SCRIPTNAME&quot; + &quot;,&quot; + &quot;RESPONSECODE&quot; + &quot;,&quot; + &quot;RESPONSEMESSAGE&quot; + &quot;,&quot; + &quot;LOADTIME&quot;);
