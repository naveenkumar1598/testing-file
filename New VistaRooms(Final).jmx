          <stringProp name="WebDriverSampler.script">import java.io.File;
import org.apache.commons.io.FileUtils;
import java.util.Random;
import openqa.selenium.OutputType.*;
import org.openqa.selenium.*;
import org.openqa.selenium.support.ui.*;
import org.openqa.selenium.support.ui.*;
import java.util.concurrent.TimeUnit;
import org.openqa.selenium.support.ui.ExpectedConditions;
import org.openqa.selenium.JavascriptExecutor;
import org.openqa.selenium.interactions.Actions;

WDS.sampleResult.sampleStart();  
WDS.browser.get(&quot;https://www.vistarooms.com/&quot;); 
WebDriverWait wait = new WebDriverWait(WDS.browser, 30); // explicit wait
 
if(wait.until(ExpectedConditions.visibilityOfElementLocated
(By.xpath(&quot;//div[@class=&apos;row search-form&apos;]&quot;))).isDisplayed() != true) {
    WDS.sampleResult.setSuccessful(false);
}

if(wait.until(ExpectedConditions.presenceOfElementLocated
(By.xpath(&quot;//div[@id=&apos;carouselExampleControls&apos;]&quot;))).isDisplayed() != true) {
    WDS.sampleResult.setSuccessful(false);
}
  
if(wait.until(ExpectedConditions.presenceOfElementLocated
(By.xpath(&quot;//div[@class=&apos;owl-item active&apos;][1]&quot;))).isDisplayed() != true) {
    WDS.sampleResult.setSuccessful(false);
}

WDS.sampleResult.sampleEnd();</stringProp>
